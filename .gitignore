# Configuration files
/.bundle
vendor/bundle/
vendor/cache/

# Generated files
log/
public/stylesheets/*.css

# Uploaded files and local files
public/uploads/
public/assets/
tmp/**/
tmp/
*.sqlite3
/db/*.sqlite3
/db/*.sqlite3-journal

# Ignore all logfiles and tempfiles.
/log/*
!/log/.keep
/tmp
